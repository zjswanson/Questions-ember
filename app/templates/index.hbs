
<div class="row question-form">
  {{question-add addQuestion="addQuestion"}}
</div>
<div class="row question-list">
  <h2>Question List!</h2>
  {{#each model as |question|}}
    {{#link-to 'question' question.id}}
    {{question-display-short question=question}}
    {{/link-to}}
  {{/each}}
</div>
